{
  "name": "mediapoint",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:local": "ng build",
    "build:staging": "ng build --configuration=staging --output-hashing=all",
    "build:rc": "ng build --configuration=rc --output-hashing=all",
    "build:prod": "ng build --configuration=production --output-hashing=all",
    "start:ssr:staging": "ng run mediapoint:server:staging",
    "start:ssr:rc": "ng run mediapoint:server:rc",
    "start:ssr:prod": "ng run mediapoint:server:production",
    "build:ssr:staging": "npm run build:staging && npm run start:ssr:staging",
    "build:ssr:rc": "npm run build:rc && npm run start:ssr:rc",
    "build:ssr:prod": "npm run build:prod && npm run start:ssr:prod",
    "dev:ssr": "ng run mediapoint:serve-ssr",
    "serve:ssr": "node dist/mediapoint/server/main.js",
    "build:ssr": "ng build && ng run mediapoint:server",
    "prerender": "ng run mediapoint:prerender",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.0",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "14.2.0",
    "@angular/compiler": "14.2.0",
    "@angular/core": "14.2.0",
    "@angular/forms": "14.2.0",
    "@angular/google-maps": "^14.0.0",
    "@angular/material": "^14.2.7",
    "@angular/material-moment-adapter": "^14.2.7",
    "@angular/platform-browser": "14.2.0",
    "@angular/platform-browser-dynamic": "14.2.0",
    "@angular/platform-server": "14.2.0",
    "@angular/router": "14.2.0",
    "@angular/service-worker": "14.2.0",
    "@ngneat/hot-toast": "^4.1.1",
    "@ngneat/overview": "^3.0.0",
    "@nguniversal/express-engine": "^14.2.3",
    "@nicky-lenaers/ngx-scroll-to": "^14.0.0",
    "bootstrap": "5.1.3",
    "chart.js": "^3.9.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "express": "^4.15.2",
    "file-saver": "^2.0.5",
    "moment": "^2.30.1",
    "ng2-charts": "^3.1.2",
    "ng2-pdfjs-viewer": "^14.0.0",
    "ngx-cookie-service-ssr": "^16.1.0",
    "pusher-js": "^8.3.0",
    "rxjs": "7.5.5",
    "swiper": "^11.1.14",
    "tslib": "2.4.0",
    "tus-js-client": "^3.1.3",
    "zone.js": "0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.13",
    "@angular-eslint/builder": "^14.4.0",
    "@angular-eslint/eslint-plugin": "14.0.0",
    "@angular-eslint/eslint-plugin-template": "14.0.0",
    "@angular-eslint/schematics": "14.0.0",
    "@angular-eslint/template-parser": "14.0.0",
    "@angular/cli": "^14.2.13",
    "@angular/compiler-cli": "14.2.0",
    "@nguniversal/builders": "^14.2.3",
    "@types/express": "^4.17.0",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "4.0.3",
    "@types/node": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "eslint": "8.18.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "2.26",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "4.2.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "localstorage-polyfill": "^1.0.1",
    "postcss": "^8.4.41",
    "postcss-less": "^6.0.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.16.1",
    "stylelint-config-standard": "^28.0.0",
    "typescript": "4.7.4"
  }
}
